From: Cappy Ishihara <cappy@cappuchino.xyz>
Date: Wed, 23 Feb 2022 09:45:59 +0700
Subject: [PATCH] default background

---
 src/wm/20_solus-project.budgie.wm.gschema.override | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/src/wm/20_solus-project.budgie.wm.gschema.override b/src/wm/20_solus-project.budgie.wm.gschema.override
index 734eaa8e..e1787953 100644
--- a/src/wm/20_solus-project.budgie.wm.gschema.override
+++ b/src/wm/20_solus-project.budgie.wm.gschema.override
@@ -3,13 +3,13 @@ edge-tiling = true
 attach-modal-dialogs = true
 
 [org.gnome.desktop.background:Budgie]
-picture-uri = 'file:///usr/share/backgrounds/budgie/default.jpg'
+picture-uri = 'file:///usr/share/backgrounds/ultramarine-linux/default/default.jpg'
 
 [org.gnome.desktop.wm.preferences:Budgie]
 button-layout = 'appmenu:minimize,maximize,close'
 
 [org.gnome.desktop.screensaver:Budgie]
-picture-uri = 'file:///usr/share/backgrounds/budgie/default.jpg'
+picture-uri = 'file:///usr/share/backgrounds/ultramarine-linux/default/default.jpg'
 
 [org.gnome.desktop.wm.keybindings:Budgie]
 switch-input-source = ['<Alt>Shift_L', '<Super>space', 'XF86Keyboard']
